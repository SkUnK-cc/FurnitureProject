<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.com.furnitureproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".FurnitureApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AccountAddActivity" />
        <activity android:name=".activity.GoodsAddActivity" />
        <activity android:name=".activity.AnalyzeSetting" />
        <activity android:name=".activity.AccountEditActivity" />
        <activity android:name=".activity.GoodsListActivity" />
        <activity android:name=".activity.OtherPayoutListActivity"></activity>
    </application>

</manifest>